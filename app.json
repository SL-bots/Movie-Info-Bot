    "repository": "https://github.com/SL-bots/Movie-Info-Bot",
    "keywords": [
        "movie",
        "information",
        "finder",
        "pyrogram",
        "telegram bot"
    ],
    "env": {
        "BOT_TOKEN": {
            "description": "Your Bot token from @Botfather"
